[user]
  name = Matheus Oliveira Costa
  email = matheusoliveiracos@gmail.com
[alias]
  st = status
  ci = commit
  a = add
  ps = push
  pl = pull
  co = checkout
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  this = !git init && git add . && git commit -m \"First commit\"
  branches = branch -a --sort=-committerdate --format=\"%(color:blue)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\"
[color]
  ui = true
  status = auto
  branch = auto
[github]
  user = mathocosta
[core]
  autocrlf = true
  excludesfile = /Users/mathocosta/.gitignore_global
[gui]
  encoding = utf-8
[i18n]
  commitencoding = utf-8
  logoutputencoding = utf-8
[color "status"]
  added = yellow
  changed = green
  untracked = cyan